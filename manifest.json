{
    "manifest_version": 3,
    "name": "UIT Extension",
    "version": "1.0",
    "description": "UIT Extension",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    
    "action":{
      "default_popup": "popup.html"
    },
    "content_scripts": [
    {
    "matches": ["https://www.overleaf.com/*"],
    "js": ["content-script.js",
    "background.js"
  ]
    }
    ],
    "permissions":[
      "identity"
    ]
}
  

